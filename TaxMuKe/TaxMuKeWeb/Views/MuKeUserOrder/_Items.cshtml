@model IPagedList<BasicData.Domain.MuKeUserOrder>
    @{
        Layout = "~/Views/Shared/_ItemsPartialLayout.cshtml";
    }

    @section Head
{
        <th>ID</th>
        <th>订单号</th>
        <th>用户</th>
        <th>手机号</th>
        <th>交易内容</th>
        <th>支付宝交易号</th>
        <th>支付宝帐号</th>
        <th>交易金额</th>
        <th>交易状态</th>
        <th>付款时间</th>
    }

    @section Items
{
        @foreach (var item in Model)
    {
            <tr class="hover">
                <td>@item.Id</td>
                <td>@item.out_trade_no</td>
                <td>@item.User.LoginName</td>
                <td>@item.User.Phone</td>
                <td>@item.subject</td>
                <td>@item.trade_no</td>
                <td>@item.buyer_email</td>
                <td>@item.Money</td>
                <td>@Html.EnumDescription(item.MuKeUserOrderStatus)</td>
                @if (item.gmt_payment.HasValue)
                {
                    <td>@item.gmt_payment.Value.ToString("yyyy-MM-dd HH:mm")</td>
                }
                else
                {
                    <td></td>
                }
              
            </tr>
        }
    }
