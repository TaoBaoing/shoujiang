@model IPagedList<BasicData.Domain.MuKeUserOrder>
    @{
        Layout = "~/Views/Shared/_ListPageLayout.cshtml";
        ViewBag.Title = "订单管理";
    }
    @section StylesChild{
        <link href="~/Assets/Plugins/My97DatePicker/skin/WdatePicker.css" rel="stylesheet" />
    }
    @section ScriptsChild
{
        <script src="~/Assets/Plugins/My97DatePicker/WdatePicker.js"></script>
        <script src="~/Assets/Plugins/My97DatePicker/lang/zh-cn.js"></script>
        <script type="text/javascript">
            $(function () {
                $("#MuKeUserOrderExport").click(function () {
                    window.location.href = "/MuKeUserOrder/ExportExcel?out_trade_no=" + $("#out_trade_no").val() + "&buyer_email=" + $("#buyer_email").val() + "&logicname=" + $("#logicname").val() + "&phone=" + $("#phone").val() + "&txtStartTime=" + $("#txtStartTime").val() + "&txtEndTime=" + $("#txtEndTime").val();
                })
            })
        </script>
    }
    @section SearchCondition
{
        <tr>
            <th><label>订单号</label></th>
            <td><input type="text" style="width: 80px !important" name="out_trade_no" class="txt" id="out_trade_no"/></td>

            <th><label>支付宝</label></th>
            <td><input type="text" style="width: 80px !important" name="buyer_email" class="txt" id="buyer_email"/></td>

            <th><label>用户</label></th>
            <td><input type="text" style="width: 80px !important" name="logicname" class="txt" id="logicname"/></td>

            <th><label>手机号</label></th>
            <td><input type="text" style="width: 80px !important" maxlength="11" name="phone" class="txt" id="phone"/></td>


            <th><label>支付时间</label></th>
            <td><input type="text" id="txtStartTime" style="width: 80px !important" name="txtStartTime" class="txt Wdate" onclick="WdatePicker({  });" /></td>
            <th><label>-></label></th>
            <td><input type="text" id="txtEndTime" style="width: 80px !important" name="txtEndTime" class="txt Wdate" onclick="WdatePicker({ });" /></td>
            @*<th><label>交易状态</label></th>
                <td>
                    @Html.DropDownList("UserOrderStatus", @Html.SelectListItems(MuKeUserOrderStatus.None, true))
                </td>*@

            <td><input type="submit" class="btn-search" value="查询" /></td>
            <td><input type="button" class="btn-search" value="导出Excel" id="MuKeUserOrderExport" /></td>
        </tr>
    }
