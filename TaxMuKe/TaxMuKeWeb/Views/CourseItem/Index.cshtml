@model IPagedList<MuKeCourseCapter>
@{
    Layout = "~/Views/Shared/_ListPageLayout.cshtml";
    ViewBag.Title = Request.QueryString["CourseName"];
}
@section SearchCondition
{
    <tr>
        <th><label>状态</label></th>
        <td>
            @Html.DropDownList("Status", @Html.SelectListItems(MuKeEnabledStatus.None, true))
        </td>
        <th><label>名称</label></th>
        <td><input type="text" name="Title" class="txt" /></td>
        <td><input type="hidden" name="CourseId" id="CourseId" value="@ViewBag.CourseId" /></td>
    <td><input type="submit" class="btn-search" value="查询" /></td>
        <td><a id="CourseItemAdd" href="FormForAdd?viewName=CourseItemForm&CourseId=@ViewBag.CourseId">添加</a></td>
       
    </tr>
}
@*<a href="/CourseItem/" target="_blank"></a>*@
<input type="button" class="btn-search" onclick="exportexcel()" value="导出用户观看记录"/>
<script type="text/javascript">
    function exportexcel() {
           
        window.location.href = "/CourseItem/CreateExcel?courseid=" + $("#CourseId").val();

    }
</script>

